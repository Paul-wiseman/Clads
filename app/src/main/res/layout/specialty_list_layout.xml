<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/specialty_list_layout"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <CheckBox
        android:id="@+id/specialty_list_layout_specialty_checkbox"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/margin_0dp"
        android:layout_marginTop="@dimen/margin_10dp"
        android:buttonTint="@color/deep_sky_blue"
        android:fontFamily="@font/poppins_medium"
        android:text="@string/specialty_list_layout_checkbox"
        android:textColor="@color/drawer_menu_item_textcolor"
        android:textSize="@dimen/text_size_15sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>